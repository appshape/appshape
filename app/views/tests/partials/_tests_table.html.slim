.wrapper.wrapper-content.animated.fadeInUp
  .ibox
    .ibox-title
      h5= "#{project.organization.name} / #{project.name}"
      .ibox-tools
        = link_to 'Create new test', new_test_path(organization_id: project.organization_id, project_id: project.id), class: 'btn btn-primary btn-xs'

    .ibox-content
      .project-list
        table.table.table-hover
          tbody
            = render partial: 'tests/partials/test_row', collection: tests, as: :test